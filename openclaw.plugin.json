{
  "id": "openclaw-moviepilot",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "services": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "moviepilot": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "baseUrl": {
                "type": "string",
                "title": "MoviePilot API Base URL",
                "description": "Base URL for MoviePilot API (for example, http://localhost:3001)."
              },
              "apiKey": {
                "type": "string",
                "title": "MoviePilot API Key"
              },
              "apiKeyMode": {
                "type": "string",
                "enum": ["header", "query", "bearer", "none"],
                "default": "header",
                "description": "How to send the API key."
              },
              "apiKeyHeader": {
                "type": "string",
                "default": "X-API-KEY",
                "description": "Header name for API key auth when apiKeyMode=header."
              },
              "apiKeyQueryParam": {
                "type": "string",
                "default": "apikey",
                "description": "Query parameter name for API key auth when apiKeyMode=query."
              },
              "timeoutMs": {
                "type": "integer",
                "minimum": 1000,
                "default": 15000,
                "description": "Request timeout in milliseconds."
              },
              "retries": {
                "type": "integer",
                "minimum": 0,
                "maximum": 5,
                "default": 1,
                "description": "Retry count for transient API errors."
              },
              "endpoints": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                  "searchMedia": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string", "default": "/api/v1/search/media/{mediaId}" },
                      "method": { "type": "string", "default": "GET" }
                    }
                  },
                  "searchTitle": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string", "default": "/api/v1/search/title" },
                      "method": { "type": "string", "default": "GET" }
                    }
                  },
                  "downloadsList": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string", "default": "/api/v1/download/" },
                      "method": { "type": "string", "default": "GET" }
                    }
                  },
                  "downloadsAdd": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string", "default": "/api/v1/download/" },
                      "method": { "type": "string", "default": "POST" }
                    }
                  },
                  "downloadsPause": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string", "default": "/api/v1/download/stop/{hash}" },
                      "method": { "type": "string", "default": "GET" }
                    }
                  },
                  "downloadsResume": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string", "default": "/api/v1/download/start/{hash}" },
                      "method": { "type": "string", "default": "GET" }
                    }
                  },
                  "downloadsRemove": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string", "default": "/api/v1/download/{hash}" },
                      "method": { "type": "string", "default": "DELETE" }
                    }
                  },
                  "subscriptionsList": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string" },
                      "method": { "type": "string", "default": "GET" }
                    }
                  },
                  "subscriptionsAdd": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string" },
                      "method": { "type": "string", "default": "POST" }
                    }
                  },
                  "subscriptionsRemove": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                      "path": { "type": "string" },
                      "method": { "type": "string", "default": "DELETE" }
                    }
                  }
                }
              },
              "debug": {
                "type": "boolean",
                "default": false
              }
            }
          }
        }
      }
    }
  },
  "skills": ["skills/moviepilot"]
}
